(()=>{let article=document.querySelector("article.cosmos");const scrollHeightThirtyPercent=~~((30/100)*document.body.scrollHeight);window.addEventListener("scroll",(handler=()=>{if(window.innerHeight+window.scrollY>=scrollHeightThirtyPercent){window.removeEventListener("scroll",handler);setTimeout(()=>{if(window.innerHeight+window.scrollY>=scrollHeightThirtyPercent){let dataLayerAlura=window.dataLayerAlura||[];dataLayerAlura.push({event:"post_read",name:article.dataset.ga4ArticleName,category:article.dataset.ga4ArticleCategory,authors:article.dataset.ga4ArticleAuthor.split(","),})}else{window.addEventListener("scroll",handler)}},5000)}}))})()